;; Macro moins triviale: `fun` avec arguments inversés! (de exemples2.psil)
(def revfun
     (macro (fun body (moremacro
                      (fun arg (cons (cons 'fun arg) body))))))

(def r9 (: (revfun (+ x 7) x) (Int -> Int)))

